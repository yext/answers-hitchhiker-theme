ANSWERS.addComponent('SearchBar', Object.assign({{{ json componentSettings.SearchBar }}}, {
  container: '#standard-searchbar-container',
  {{#if componentSettings.SearchBar.verticalKey}}
    verticalKey: '{{componentSettings.SearchBar.verticalKey}}',
  {{else}}
    {{#if currentVertical}}
      verticalKey: '{{currentVertical}}',
    {{/if}}
  {{/if}}
}));