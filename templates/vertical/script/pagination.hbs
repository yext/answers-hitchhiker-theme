ANSWERS.addComponent('Pagination', Object.assign({{{ json componentSettings.Pagination }}}, {
  container: '.js-Answers-pagination',
  {{#if componentSettings.Pagination.verticalKey}}
    verticalKey: '{{componentSettings.Pagination.verticalKey}}',
  {{else}}
    {{#if currentVertical}}
      verticalKey: '{{currentVertical}}',
    {{/if}}
  {{/if}}
}));
